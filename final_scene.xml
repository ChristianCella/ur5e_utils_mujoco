                  <geom name="wrist_3_collision" class="eef_collision" pos="0 0.08 0" quat="1 1 0 0" size="0.04 0.02"/>
                  <site name="ee_site" pos="0 0.1 0" quat="0.7071 -0.7071 0 0"/>
                  <body name="ee_frame_visual_only" pos="0 0.1 0" quat="0.7071 -0.7071 0 0">
                      <geom type="capsule" fromto="0 0 0 0.05 0 0" rgba="1 0 0 1" size="0.002" contype="0" conaffinity="0"/>
                      <geom type="capsule" fromto="0 0 0 0 0.05 0" rgba="0 1 0 1" size="0.002" contype="0" conaffinity="0"/>
                      <geom type="capsule" fromto="0 0 0 0 0 0.05" rgba="0 0 1 1" size="0.002" contype="0" conaffinity="0"/>
                      <!-- The tool is imported by a python code -->
                      
                      
                      <!-- Reference frame attached @ tool tip for screwing (T) -->
                      <body name="tool_frame" pos="0 0 0" quat="1 0 0 0">
                        <site name="tool_site" pos="0 0 0" quat="1 0 0 0"/>
                        <geom type="capsule" fromto="0 0 0 0.06 0 0" rgba="1 0 0 1" size="0.003" contype="0" conaffinity="0"/>
                        <geom type="capsule" fromto="0 0 0 0 0.06 0" rgba="0 1 0 1" size="0.003" contype="0" conaffinity="0"/>
                        <geom type="capsule" fromto="0 0 0 0 0 0.06" rgba="0 0 1 1" size="0.003" contype="0" conaffinity="0"/>
                    </body>
                  <body name="tool_top" pos="0 0 0">
      <inertial mass="2.6" pos="0 0 0.155" diaginertia="0.0143 0.0143 0.0016"/>
      <geom type="mesh" mesh="tool_mesh" rgba="0.2 0.2 0.2 1" contype="0" conaffinity="0" group="0"/>
      <!-- collision cylinder -->
      <geom name="screwdriver" type="cylinder" size="0.035 0.155" pos="0 0 0.155" rgba="0.2 0.8 0.2 1" contype="1" conaffinity="1" group="3"/>
      <!-- Marker (red sphere) to specify the origin -->
      <site name="mount_site" pos="0 0 0" size="0.005" rgba="1 0 0 1"/>
      <!-- Marker (green sphere) to specify the point of application of the weight -->
      <geom type="sphere" size="0.005" pos="0 0 0.125" rgba="0 1 0 1" contype="0" conaffinity="0" group="2"/>
    </body>
  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  <body name="table_grip" pos="0 0 0">

      <geom type="mesh" mesh="table_grip_mesh" rgba="0.45 0.45 0.45 1" contype="0" conaffinity="0" group="0"/>

      <!-- Base plate -->
