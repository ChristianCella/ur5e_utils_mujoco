        <geom name="world_z_axis" type="capsule" fromto="0 0 0 0 0 0.15" size="0.01" rgba="0 0 1 1" contype="0" conaffinity="0"/>
    </body>

  <light name="spotlight" mode="targetbodycom" target="wrist_2_link" pos="0 -1 2"/>
